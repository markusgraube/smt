DELETE{
    ?s ?p ?o.
}
# SELECT DISTINCT  *
WHERE{
    {
        ?s a c:Mark.
        LET ( ?p := rdf:type)
        LET ( ?o := c:Mark)
    }
    UNION
    {
        ?s a c:Copy.
        LET ( ?p := rdf:type)
        LET ( ?o := c:Copy)
    }
    UNION
    {
        ?s c:ref ?o.
        LET ( ?p := c:ref)
    }
    
}

